public class LabThree {

	public static void main(String[] args) {
		
		int y1,y2 ;
		
		System.out.printf("%6s%11s%n", "Y1","Y2") ;
		System.out.println("---------------------");
		for (int i = 0 ; i < 101 ; i+=10) {
			y1 = 100*i+10 ;
			y2 = 5*i*i+2 ;
			
			
			System.out.printf("%-10s%s%10s%n",y1,"|",y2);
		}
	}
	

}
